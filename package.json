    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-typescript": "^0.14.0",
    "nodemon": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.17.0",
    "ts-node": "^10.9.2"
  }
}
