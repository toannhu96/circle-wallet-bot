// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model CircleWallet {
  id             String     @id @default(uuid()) @db.VarChar
  telegramId     String
  walletId       String
  walletSetId    String
  createdAt      BigInt

  @@map("circle_wallets")
}